<view class="container data-v-9143e25a"><block wx:if="{{!hasLogin||empty===true}}"><view class="empty data-v-9143e25a"><image src="../../static/emptyCart.jpg" class="data-v-9143e25a"></image><block wx:if="{{hasLogin}}"><view class="empty-tips data-v-9143e25a">空空如也<navigator class="navigator data-v-9143e25a" url="../index/index" open-type="switchTab">随便逛逛</navigator></view></block><block wx:else><view class="empty-tips data-v-9143e25a">空空如也<view data-event-opts="{{[['tap',[['goLogin',['$event']]]]]}}" class="navigator data-v-9143e25a" bindtap="__e">去登录></view></view></block></view></block><block wx:else><view class="data-v-9143e25a"><view class="cart-list data-v-9143e25a"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="data-v-9143e25a"><view class="cart-item vue-1px-b data-v-9143e25a"><view class="image-wrap data-v-9143e25a"><image src="{{item.image}}" mode="aspectFill" class="data-v-9143e25a"></image><view data-event-opts="{{[['tap',[['check',['item',index]]]]]}}" class="{{['yticon icon-xuanzhong2 checkbox data-v-9143e25a',(item.checked)?'checked':'']}}" bindtap="__e"></view></view><view class="info-wrap data-v-9143e25a"><text class="title data-v-9143e25a">{{item.title}}</text><text class="type data-v-9143e25a">{{item.attr_val}}</text><text class="price data-v-9143e25a">{{"¥"+item.price}}</text><uni-number-box class="step data-v-9143e25a" vue-id="{{'3216da80-1-'+index}}" min="{{1}}" max="{{item.stock}}" value="{{item.number>item.stock?item.stock:item.number}}" index="{{index}}" data-event-opts="{{[['^change',[['numberChange']]]]}}" bind:change="__e" bind:__l="__l"></uni-number-box></view><text data-event-opts="{{[['tap',[['deleteCartItem',[index]]]]]}}" class="del-btn yticon icon-fork data-v-9143e25a" bindtap="__e"></text></view></block></block></view><view class="action-section data-v-9143e25a"><view class="checkbox data-v-9143e25a"><image src="{{allChecked?'/static/selected.png':'/static/select.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['check',['all']]]]]}}" bindtap="__e" class="data-v-9143e25a"></image><view data-event-opts="{{[['tap',[['clearCart',['$event']]]]]}}" class="{{['clear-btn data-v-9143e25a',(allChecked)?'show':'']}}" bindtap="__e">清空</view></view><view class="total-box data-v-9143e25a"><text class="price data-v-9143e25a">{{"¥"+total}}</text><text class="coupon data-v-9143e25a">已优惠<text class="data-v-9143e25a">56.34</text>元</text></view><button class="calc-btn data-v-9143e25a" type="primary" data-event-opts="{{[['tap',[['creatOrder',['$event']]]]]}}" bindtap="__e">去结算</button></view></view></block></view>